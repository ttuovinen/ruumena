{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react",
        "react-hooks"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "prettier/prettier": ["warn"],
        "import/prefer-default-export": ["off"],
        "react/jsx-curly-newline": ["off"],
        "react/jsx-wrap-multilines": ["off"],
        "react/jsx-one-expression-per-line": "off"
    }
}
